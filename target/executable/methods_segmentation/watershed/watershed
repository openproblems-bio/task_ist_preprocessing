  echo "        type: boolean"
  echo "        default: false"
  echo ""
  echo "    --threshold_footprint"
  echo "        type: string"
  echo "        default: square"
  echo ""
  echo "    --threshold_footprint_size"
  echo "        type: integer"
  echo "        default: 50"
  echo ""
  echo "    --distance_transform_func"
  echo "        type: string"
  echo "        default: distance_transform_edt"
  echo ""
  echo "    --distance_transform_sampling"
  echo "        type: string"
  echo "        default: None"
  echo ""
