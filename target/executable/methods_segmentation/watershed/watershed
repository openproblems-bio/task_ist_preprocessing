  echo ""
  echo "    --distance_transform_return_indices"
  echo "        type: boolean"
  echo "        default: false"
  echo ""
  echo "    --distance_transform_distances"
  echo "        type: string"
  echo "        default: None"
  echo ""
  echo "    --distance_transform_indices"
  echo "        type: string"
  echo "        default: None"
  echo ""
  echo "    --local_maxima_func"
  echo "        type: string"
  echo "        default: find_local_maxima"
  echo ""
  echo "    --local_maxima_min_distance"
  echo "        type: integer"
  echo "        default: 5"
  echo ""
  echo "    --post_processing_func_1"
  echo "        type: string"
  echo "        default: remove_small_objects"
  echo ""
  echo "    --post_processing_min_size_1"
  echo "        type: integer"
  echo "        default: 64"
  echo ""
  echo "    --post_processing_connectivity_1"
  echo "        type: integer"
  echo "        default: 1"
  echo ""
  echo "    --post_processing_out_1"
  echo "        type: string"
  echo "        default: None"
  echo ""
  echo "    --post_processing_func_2"
  echo "        type: string"
  echo "        default: remove_small_holes"
  echo ""
  echo "    --post_processing_area_threshold_2"
  echo "        type: integer"
  echo "        default: 64"
  echo ""
  echo "    --post_processing_connectivity_2"
  echo "        type: integer"
  echo "        default: 1"
  echo ""
  echo "    --post_processing_out_2"
  echo "        type: string"
  echo "        default: None"
  echo ""
  echo "    --bg_intensity_filter_bg_factor"
  echo "        type: double"
  echo "        default: 0.3"
  echo ""
  echo "    --bg_intensity_filter_window_size"
  echo "        type: integer"
  echo "        default: 1000"
  echo ""
  echo "    --bg_intensity_filter_bg_size"
  echo "        type: integer"
  echo "        default: 2000"
  echo ""
  echo "Viash built in Computational Requirements:"
  echo "    ---cpus=INT"
  echo "        Number of CPUs to use"
  echo "    ---memory=STRING"
  echo "        Amount of memory to use. Examples: 4GB, 3MiB."
  echo ""
  echo "Viash built in Docker:"
  echo "    ---setup=STRATEGY"
  echo "        Setup the docker container. Options are: alwaysbuild, alwayscachedbuild, ifneedbebuild, ifneedbecachedbuild, alwayspull, alwayspullelsebuild, alwayspullelsecachedbuild, ifneedbepull, ifneedbepullelsebuild, ifneedbepullelsecachedbuild, push, pushifnotpresent, donothing."
  echo "        Default: ifneedbepullelsecachedbuild"
  echo "    ---dockerfile"
  echo "        Print the dockerfile to stdout."
  echo "    ---docker_run_args=ARG"
  echo "        Provide runtime arguments to Docker. See the documentation on \`docker run\` for more information."
  echo "    ---docker_image_id"
  echo "        Print the docker image id to stdout."
  echo "    ---debug"
  echo "        Enter the docker container for debugging purposes."
  echo ""
  echo "Viash built in Engines:"
  echo "    ---engine=ENGINE_ID"
  echo "        Specify the engine to use. Options are: docker, native."
  echo "        Default: docker"
